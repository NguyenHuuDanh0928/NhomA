@model IEnumerable<WebDichVu.Datas.LichSuDatDichVu>

@{
    ViewData["Title"] = "LichSu";
    Layout = "~/Areas/Admin/Views/Shared/LayoutAdmin.cshtml";
}

<h1>Lịch Sử Đặt Dịch Vụ</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Thời Gian
            </th>
            <th>
                Trạng Thái Dịch Vụ
            </th>
            <th>
                Dịch Vụ Được Đặt
            </th>
            <th>
                Khách Hàng
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ThoiGian)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrangThai)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DichVu.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KhachHang.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
